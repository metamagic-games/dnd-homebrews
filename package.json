{
  "name": "handbooker-sample-project",
  "version": "1.1.1",
  "description": "Hammerheim rules project",
  "main": "index.js",
  "scripts": {
    "build": "babel-node scripts/createRulebooks",
    "watch": "watch 'npm run build' ./src --wait=1",
    "format": "prettier --write scripts/ src/",
    "format:staged": "pretty-quick --staged --pattern ./scripts/**",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --bail"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:staged && echo Did you remember to build the CV?"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hammerheim-game/hammerheim-rules.git"
  },
  "keywords": [
    "hammerheim",
    "rules",
    "mordheim",
    "warhammer",
    "tabletop",
    "game"
  ],
  "author": "Max Clayton Clowes <maxcc@me.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hammerheim-game/hammerheim-sample-project/issues"
  },
  "homepage": "https://github.com/hammerheim-game/hammerheim-sample-project#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "handbooker": "^1.6.6",
    "husky": "^4.3.6",
    "jest": "^26.6.1",
    "lodash": "^4.17.20",
    "marked": "^1.2.2",
    "merge-md": "^1.0.3",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "watch": "^1.0.2"
  }
}
